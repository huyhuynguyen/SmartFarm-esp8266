<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivityControllDevice">

    <Button
        android:id="@+id/btnServo"
        android:layout_width="134dp"
        android:layout_height="59dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="48dp"
        android:text="SERVO OFF"
        android:textColor="#100F0F"
        app:backgroundTint="#CFD3D2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSettime_light"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnSettime_servo"
        android:layout_width="139dp"
        android:layout_height="49dp"
        android:layout_marginStart="136dp"
        android:layout_marginBottom="32dp"
        android:text="Setting"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/servo_time_end"
        app:layout_constraintVertical_bias="0.375"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/servo_time_start"
        android:layout_width="78dp"
        android:layout_height="25dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="21:06:00"
        app:layout_constraintEnd_toEndOf="@+id/btnServo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/btnServo"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="End at:"
        app:layout_constraintEnd_toStartOf="@+id/servo_time_end"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Start at:"
        app:layout_constraintStart_toStartOf="@+id/btnServo"
        app:layout_constraintTop_toBottomOf="@+id/btnServo" />

    <TextView
        android:id="@+id/servo_time_end"
        android:layout_width="78dp"
        android:layout_height="26dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="06:10:00"
        app:layout_constraintEnd_toEndOf="@+id/servo_time_start"
        app:layout_constraintStart_toStartOf="@+id/servo_time_start"
        app:layout_constraintTop_toBottomOf="@+id/servo_time_start" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="Start at:"
        app:layout_constraintStart_toStartOf="@+id/btnLight"
        app:layout_constraintTop_toBottomOf="@+id/btnLight" />

    <Button
        android:id="@+id/btnSettime_light"
        android:layout_width="139dp"
        android:layout_height="49dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="40dp"
        android:text="Setting"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/light_time_end" />

    <Button
        android:id="@+id/btnPump"
        android:layout_width="134dp"
        android:layout_height="59dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="76dp"
        android:text="PUMP OFF"
        android:textColor="#100F0F"
        app:backgroundTint="#CFD3D2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ismanual" />

    <Button
        android:id="@+id/btnLight"
        android:layout_width="134dp"
        android:layout_height="59dp"
        android:layout_marginStart="52dp"
        android:text="LIGHT OFF"
        android:textColor="#000000"
        app:backgroundTint="#CFD3D2"
        app:layout_constraintStart_toEndOf="@+id/btnPump"
        app:layout_constraintTop_toTopOf="@+id/btnPump" />

    <Button
        android:id="@+id/btnSettime_pump"
        android:layout_width="139dp"
        android:layout_height="49dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="4dp"
        android:text="Setting"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pump_time_end" />

    <TextView
        android:id="@+id/pump_time_start"
        android:layout_width="78dp"
        android:layout_height="25dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="21:06:00"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/btnPump" />

    <TextView
        android:id="@+id/pump_time_end"
        android:layout_width="78dp"
        android:layout_height="26dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="06:10:00"
        app:layout_constraintEnd_toEndOf="@+id/pump_time_start"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/pump_time_start"
        app:layout_constraintTop_toBottomOf="@+id/pump_time_start" />

    <TextView
        android:id="@+id/light_time_start"
        android:layout_width="78dp"
        android:layout_height="25dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="17:44:00"
        app:layout_constraintEnd_toEndOf="@+id/btnLight"
        app:layout_constraintTop_toBottomOf="@+id/btnLight" />

    <TextView
        android:id="@+id/light_time_end"
        android:layout_width="78dp"
        android:layout_height="25dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="07:00:00"
        app:layout_constraintEnd_toEndOf="@+id/light_time_start"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/light_time_start"
        app:layout_constraintTop_toBottomOf="@+id/light_time_start" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="Start at:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPump" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="End at:"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="12dp"
        android:gravity="center"
        android:text="End at:"
        app:layout_constraintEnd_toStartOf="@+id/light_time_end"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="221dp"
        android:layout_height="41dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="Control Devices"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Switch
        android:id="@+id/ismanual"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:text="Manual Control"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

</androidx.constraintlayout.widget.ConstraintLayout>